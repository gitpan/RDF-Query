@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt:     <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .

<>  rdf:type mf:Manifest ;
    rdfs:comment "Eval tests : nested constraints" ;
    mf:entries
    ( 
      [  mf:name    "OPTIONAL-AND" ;
	 rdfs:comment "OPTIONAL query with local AND constriant" ;
         mf:action
            [ qt:query  <expr-1.rq> ;
              qt:data   <data-1.ttl> ] ;
         mf:result  <expr-1-result.ttl>
      ]
      [  mf:name    "OPTIONAL - Outer AND" ;
	 rdfs:comment "OPTIONAL query with global AND constriant" ;
         mf:action
            [ qt:query  <expr-2.rq> ;
              qt:data   <data-1.ttl> ] ;
         mf:result  <expr-2-result.ttl>
      ]
      [  mf:name    "OPTIONAL - Outer AND with BOUND" ;
	 rdfs:comment "OPTIONAL query with global AND constriant and BOUND checking" ;
         mf:action
            [ qt:query  <expr-3.rq> ;
              qt:data   <data-1.ttl> ] ;
         mf:result  <expr-3-result.ttl>
      ]
    ).
