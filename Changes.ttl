<http://kasei.us/code/files/RDF-Query-1.042.tar.gz>
        dcterms:isVersionOf <http://kasei.us/code/rdf-query/#project> ;
        dcterms:replaces    <http://kasei.us/code/files/RDF-Query-1.041.tar.gz> ;

