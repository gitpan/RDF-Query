                        asc:update        "SPARQL parser now supports ORDER BY operands: variable, expression, or function call (SPARQL Draft 2005.07.21)." ;
                        ] .


<http://kasei.us/code/files/RDF-Query-1.026.tar.gz>
        dcterms:isVersionOf <http://kasei.us/code/rdf-query/> ;
        dcterms:replaces    <http://kasei.us/code/files/RDF-Query-1.024.tar.gz> ;

